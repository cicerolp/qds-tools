<?xml version="1.0" encoding="utf-8"?>
<config ver="1.0">
	<input header="true">example.csv</input>
    <output>example.nds</output>
	<schema>
        <categorical index="0" type="discrete" name="genero">
            <bins>
                <bin>
                    <key>M</key>
                </bin>
                <bin>
                    <key>F</key>
                </bin>
            </bins>
        </categorical>

        <categorical index="1" type="range" name="idade">
            <bins>
                <bin>
                    <min>0</min>
                    <max>20</max>
                </bin>
                <bin>
                    <min>21</min>
                    <max>30</max>
                </bin>
                <bin>
                    <min>31</min>
                    <max>40</max>
                </bin>
                <bin>
                    <min>41</min>
                    <max>50</max>
                </bin>
                <bin>
                    <min>51</min>
                    <max>60</max>
                </bin>
                <bin>
                    <min>61</min>
                    <max>70</max>
                </bin>
            </bins>
        </categorical>

        <categorical index="2" type="binary" name="status">
        </categorical>


        <categorical index="3" type="sequential" name="origem">
            <bins>
                <bin>
                    <min>0</min>
                    <max>9</max>
                </bin>                
            </bins>
        </categorical>

        <temporal index="4" name="data">
            <bins>
                <bin>
                    <format>%d/%m/%Y-%H:%M</format>
                    <min>01/01/2016</min>
                    <interval>86400</interval>
                    <max>31/12/2020</max>
                </bin>                
            </bins>
        </temporal>

        <spatial index="5" name="geolocation">
            <bins>
                <bin></bin>                
            </bins>
        </spatial>
	</schema>
</config>
